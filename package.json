{
  "name": "gen-json-schema",
  "version": "1.0.0",
  "description": "Infer a JSON Schema from JSON values",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "circular": "madge --circular --extensions ts src",
    "pretest:lcov": "node --loader ts-node/esm scripts/test-runner-node-version.ts",
    "test:lcov": "glob -c \"node --experimental-test-coverage --test-reporter=spec --test-reporter=lcov --test-reporter-destination=stdout --test-reporter-destination=lcov.info  --loader ts-node/esm --test\" \"test/**/*.test.ts\"",
    "test": "glob -c \"node --experimental-test-coverage --loader ts-node/esm --test\" \"test/**/*.test.ts\"",
    "lint": "eslint src && npm run circular",
    "prebuild": "rimraf dist",
    "build": "rollup --config --configPlugin @rollup/plugin-typescript",
    "postbuild": "rimraf dist/dts",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robere2/gen-json-schema.git"
  },
  "bugs": {
    "url": "https://github.com/robere2/gen-json-schema/issues"
  },
  "homepage": "https://github.com/robere2/gen-json-schema",
  "author": "Erik Roberts",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "madge": "^7.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.16.4",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "keywords": [
    "JSON Schema",
    "JSONSchema",
    "Draft 7",
    "JSON",
    "Schema"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "**/*": [
      "eslint",
      "prettier --write --ignore-unknown"
    ]
  }
}
